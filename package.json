{
  "name": "report-group-backend",
  "version": "1.0.0",
  "main": "index.js",
  "jest": {
    "setupFiles": [
      "./src/__test__/lib/jest-setup.js"
    ],
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      "/__test__/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "scripts": {
    "start": "node index.js",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=http* nodemon index.js",
    "test": "jest --i",
    "test:watch": "jest --watchAll",
    "test:debug": "DEBUG=http* jest --i",
    "lint": "eslint .",
    "lint:test": "npm run lint && npm test",
    "start-db": "mkdir -p ./data/db && mongod --dbpath ./data/db",
    "stop-db": "killall mongod"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "jest": "^22.4.2",
    "request": "^2.83.0",
    "superagent": "^3.8.2"
  },
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "http-errors": "^1.6.2",
    "json-parser": "^1.1.5",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.1",
    "webpack": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/restore-master/back-end.git"
  },
  "author": "Koko Kassa, Mitchel Massie, Joel Clair",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/restore-master/back-end/issues"
  },
  "homepage": "https://github.com/restore-master/back-end#readme",
  "description": ""
}
